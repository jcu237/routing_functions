variable_group x,y,z,t;
function f1,f2,f3,f4;

f1 = -8*((-2*x + 4*x^3)^2 + (-2*y + 4*y^3)^2 + (-3*z^2 + 4*z^3)^2)*(-0.7978234324 + x) - t*(-2*x + 4*x^3)*(1 + (-0.7978234324 + x)^2 + (-0.6623073432 + y)^2 + (-0.2347907832 + z)^2)^5 + 2*(-2*x + 4*x^3)*(-2 + 12*x^2)*(1 + (-0.7978234324 + x)^2 + (-0.6623073432 + y)^2 + (-0.2347907832 + z)^2);
f2 = -8*((-2*x + 4*x^3)^2 + (-2*y + 4*y^3)^2 + (-3*z^2 + 4*z^3)^2)*(-0.6623073432 + y) - t*(-2*y + 4*y^3)*(1 + (-0.7978234324 + x)^2 + (-0.6623073432 + y)^2 + (-0.2347907832 + z)^2)^5 + 2*(-2*y + 4*y^3)*(-2 + 12*y^2)*(1 + (-0.7978234324 + x)^2 + (-0.6623073432 + y)^2 + (-0.2347907832 + z)^2);
f3 = -8*((-2*x + 4*x^3)^2 + (-2*y + 4*y^3)^2 + (-3*z^2 + 4*z^3)^2)*(-0.2347907832 + z) - (-3*z^2 + 4*z^3)*t*(1 + (-0.7978234324 + x)^2 + (-0.6623073432 + y)^2 + (-0.2347907832 + z)^2)^5 + 2*(-3*z^2 + 4*z^3)*(-6*z + 12*z^2)*(1 + (-0.7978234324 + x)^2 + (-0.6623073432 + y)^2 + (-0.2347907832 + z)^2);
f4 = 0.5 - (x^2 + y^2 + z^3) + x^4 + y^4 + z^4;
END;
